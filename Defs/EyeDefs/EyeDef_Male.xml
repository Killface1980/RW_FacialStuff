<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <FacialStuff.Defs.EyeDef>
  	<!-- 
	Texture name is built like so:
	[texCollection]_[texName]_[direction].png
	-->
    <defName>Eye_Male_Average_FL</defName>
	<label>Human Male Average Flat</label>
	<!-- 
	texCollection is used to limit eye types for each races. Whether the race can use the 
	texCollection is specified in CompProperties_Face.
	-->
	<texCollection>Human</texCollection>
    <hairGender>Male</hairGender>
    <texBasePath></texBasePath> <!-- empty for default path "Things/Pawn/Humanlike/Eyes/"-->
    <texName>MFL</texName> <!-- The eye name. File name of the textures is always "Eye_<texName>_<pawnGender_Left/Right_east"  -->
	<closedEyeDef>Closed</closedEyeDef>
	<missingEyeDef>Missing</missingEyeDef>
    <hairTags>
      <li>Urban</li>
      <li>Rural</li>
      <li>Punk</li>
      <li>Tribal</li>
    </hairTags>
  </FacialStuff.Defs.EyeDef>

