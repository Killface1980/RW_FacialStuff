<?xml version="1.0" encoding="utf-8"?>
<Defs>
	
	<PawnPlus.Defs.PartDef>
		<defName>FSHumanFFL</defName>
		<label>Human Female Flat</label>
		<raceSettings>
			<li>
				<!-- defName of BodyDef -->
				<key>Human</key>
				<value>
					<hairGender>FemaleUsually</hairGender>
					<hairTags>
						<li>Urban</li>
						<li>Rural</li>
						<li>Punk</li>
						<li>Tribal</li>
					</hairTags>
					<category>Eyes</category>
					<linkedBodyParts>
						<li>
							<bodyPartLocator>
								<bodyPartDef>Eye</bodyPartDef>
								<bodyPartLabel>left eye</bodyPartLabel>
							</bodyPartLocator>
							<attachment>Head</attachment>
							<!-- Class must implement PawnPlus.IGraphicProvider interface. -->
							<graphicProvider Class="PawnPlus.HumanEyeGraphicProvider">
								<closeWhenAiming>true</closeWhenAiming>
								<additionalOffset>(0, 0, 0)</additionalOffset>
							</graphicProvider>
						</li>
						<li>
							<bodyPartLocator>
								<bodyPartDef>Eye</bodyPartDef>
								<bodyPartLabel>right eye</bodyPartLabel>
							</bodyPartLocator>
							<attachment>Head</attachment>
							<graphicProvider Class="PawnPlus.HumanEyeGraphicProvider">
								<closeWhenAiming>false</closeWhenAiming>
								<additionalOffset>(0, 0, 0)</additionalOffset>
							</graphicProvider>
						</li>
					</linkedBodyParts>
				</value>
			</li>
		</raceSettings>
		<namedTexPaths>
			<li>
				<!-- 
				Key (or texture name) is passed to the IGraphicProvider implementation as string during initialization. 
				It's up to the IGraphicProvider implementation to decide what the name means and when the name's 
				corresponding texture should be used. 
				PawnPlus.HumanEyeGraphicProvider can accept the following names:
					Open, Closed, Dead, Missing, InPain, Aiming
				-->
				<key>Open</key>
				<value>Things/Pawn/Humanlike/Eyes/HumanFFL</value>
			</li>
			<li>
				<key>Closed</key>
				<value>Things/Pawn/Humanlike/Eyes/HumanClosed</value>
			</li>
			<li>
				<key>Dead</key>
				<value>Things/Pawn/Humanlike/Eyes/HumanClosed</value>
			</li>
			<li>
				<key>Missing</key>
				<value>Things/Pawn/Humanlike/Eyes/HumanMissing</value>
			</li>
			<li>
				<key>InPain</key>
				<value>Things/Pawn/Humanlike/Eyes/HumanFFL</value>
			</li>
			<li>
				<key>Aiming</key>
				<value>Things/Pawn/Humanlike/Eyes/HumanFFL</value>
			</li>
		</namedTexPaths>
		<defaultTexPath>Things/Pawn/Humanlike/Eyes/HumanFFL</defaultTexPath>
	</PawnPlus.Defs.PartDef>
  
